                    xmlns:vm="clr-namespace:TrashTrackProjectV2.ViewModel"
                    xmlns:view="clr-namespace:TrashTrackProjectV2.View">

    <DataTemplate DataType="{x:Type vm:BerandaVM}">
        <view:Beranda/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:PesanVM}">
        <view:Pesan/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:BerlanggananVM}">
        <view:Berlangganan/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:AkunVM}">
        <view:Akun/>
    </DataTemplate>
</ResourceDictionary>